<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape android:shape="rectangle">
            <stroke
                android:color="@color/white"
                android:width="4dp"/>
            <solid android:color="@color/transparent"/>

            <corners
                android:radius="25dp">

            </corners>
        </shape>
    </item>
    <item
        android:left="40dp"
        android:right="40dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/transparent"/>
        </shape>
    </item>
    <item
        android:top="30dp"
        android:bottom="30dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/transparent"/>
        </shape>
    </item>
</layer-list>


